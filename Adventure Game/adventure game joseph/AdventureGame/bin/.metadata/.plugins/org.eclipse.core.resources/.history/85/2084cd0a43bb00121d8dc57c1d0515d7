public class room5
{
	public static void main (String[] args)
	{
		if(TextGUI.game.getText().equals("north"))
		{
			TextGUI.roomNum=9;
			TextGUI.area.setText(TextGUI.area.getText() + "nothing here");
		}
		else if(TextGUI.game.getText().equals("south"))
		{
			TextGUI.roomNum=1;
		}
		else if(TextGUI.game.getText().equals("east"))
		{
			TextGUI.roomNum=6;
		}
	}
}