public class room5
{
	public static void main (String[] args)
	{
		TextGUI.area.setText(TextGUI.area.getText() + "\n nothing interesting here");
		if(TextGUI.game.getText().equals("north"))
		{
			TextGUI.roomNum=9;
			Handeler.main(null);
		}
		else if(TextGUI.game.getText().equals("south"))
		{
			TextGUI.roomNum=1;
			Handeler.main(null);
		}
		else if(TextGUI.game.getText().equals("east"))
		{
			TextGUI.roomNum=6;
			Handeler.main(null);
		}
	}
}